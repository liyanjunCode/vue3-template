#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/vite@4.3.9_sass@1.63.6/node_modules/vite/bin/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/vite@4.3.9_sass@1.63.6/node_modules/vite/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/vite@4.3.9_sass@1.63.6/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules:/Users/yz/Desktop/others/vue3-template/node_modules:/Users/yz/Desktop/others/node_modules:/Users/yz/Desktop/node_modules:/Users/yz/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/vite@4.3.9_sass@1.63.6/node_modules/vite/bin/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/vite@4.3.9_sass@1.63.6/node_modules/vite/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/vite@4.3.9_sass@1.63.6/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules:/Users/yz/Desktop/others/vue3-template/node_modules:/Users/yz/Desktop/others/node_modules:/Users/yz/Desktop/node_modules:/Users/yz/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
