#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/esbuild@0.17.19/node_modules/esbuild/bin/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/esbuild@0.17.19/node_modules/esbuild/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/esbuild@0.17.19/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules:/Users/yz/Desktop/others/vue3-template/node_modules:/Users/yz/Desktop/others/node_modules:/Users/yz/Desktop/node_modules:/Users/yz/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/esbuild@0.17.19/node_modules/esbuild/bin/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/esbuild@0.17.19/node_modules/esbuild/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/esbuild@0.17.19/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules/.pnpm/node_modules:/Users/yz/Desktop/others/vue3-template/vite-project/node_modules:/Users/yz/Desktop/others/vue3-template/node_modules:/Users/yz/Desktop/others/node_modules:/Users/yz/Desktop/node_modules:/Users/yz/node_modules:/Users/node_modules:/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
